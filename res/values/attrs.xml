<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatingActionButton">
        <attr name="drawable" format="integer"/>
        <attr name="color" format="color"/>
        <attr name="shadowRadius" format="float"/>
        <attr name="shadowDx" format="float"/>
        <attr name="shadowDy" format="float"/>
        <attr name="shadowColor" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CompoundImageButton">
        <attr name="drawable_selected" format="integer"/>
        <attr name="drawable_unselected" format="integer"/>
        <attr name="background_color" format="color"/>
        <attr name="ripple_color" format="color"/>
    </declare-styleable>
</resources>
